@book{Favrod2021,
   abstract = {L’innovation technologique dans l’industrie financière évolue très rapidement. Les modèles d’affaires proposent des solutions spécialisées qui doivent s’adapter en fonction de la réglementation. Cet ouvrage aborde ces deux aspects qui sont étroitement liés et offre une vue d’ensemble de l’écosystème des entreprises Fintech en Suisse et de la réglementation applicable. Il retrace l’histoire de la Fintech, en décrit les modèles d’affaires en s’appuyant sur des études de cas et analyse les aspects réglementaires en Suisse. Ce livre s’adresse aussi bien aux étudiants ou aux praticiens qu’aux entrepreneurs ou aux banquiers. Il permet d’appréhender les tendances actuelles, d’identifier les possibilités en termes d’innovation et de},
   author = {Pascal Favrod-Coune and Vincent Pignon},
   city = {Genève/Zurich},
   publisher = {Schulthess Editions Romandes},
   title = {La Fintech en Suisse, collection "La pratique du droit"},
   year = {2021},
}
